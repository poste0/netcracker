<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">
    <enterprise-beans>
        <session>
            <ejb-name>MessageBean</ejb-name>
            <business-remote>interfaces.MessageBeanRemote</business-remote>
            <ejb-class>beans.MessageBean</ejb-class>
            <session-type>Stateless</session-type>
        </session>
        <session>
            <ejb-name>ShowBean</ejb-name>
            <business-remote>interfaces.ShowBeanRemote</business-remote>
            <ejb-class>beans.ShowBean</ejb-class>
            <session-type>Stateless</session-type>
        </session>

    </enterprise-beans>
</ejb-jar>
